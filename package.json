  "scripts": {
    "dev": "npm run css && webpack-dev-server --mode development --progress --colors --env dev --watch --config ./webpack.config.babel.js && nodemon -e scss -x \"npm run css\"",
    "css": "node-sass --include-path scss src/styles/styles.scss -o dist",
    "start": "npm-run-all --parallel server scss",
    "server": "webpack-dev-server --mode development --progress --colors --env dev --watch --config ./webpack.config.babel.js",
    "scss": "nodemon -e scss -x \"npm run css\""
  },
